                    <div class="d-flex align-items-start">
                        <img class="me-2" src="~/Content/Falcon/assets/img/icons/globe-asia.png" width="20" alt="" />
                        <div class="flex-1">
                            <h5 class="fs-0">Language</h5>
                            <p class="fs--1 mb-0">Select your language </p>
                        </div>
                    </div>
                    <div class="form-check form-switch"><input class="form-check-input ms-0" id="mode-rtl" type="checkbox" data-theme-control="isRTL" /></div>
                </div>
                <hr />*@
            <div class="d-flex justify-content-between">
                <div class="d-flex align-items-start">
                    <img class="me-2" src="~/Content/Falcon/assets/img/icons/arrows-h.svg" width="20" alt="" />
                    <div class="flex-1">
                        <h5 class="fs-0">@Resource.FluidLayout</h5>
                        <p class="fs--1 mb-0">@Resource.ToggleContainerLayout </p>
                    </div>
                </div>
                <div class="form-check form-switch"><input class="form-check-input ms-0" id="mode-fluid" type="checkbox" data-theme-control="isFluid" /></div>
            </div>
            <hr />
            <div class="d-flex align-items-start">
                <img class="me-2" src="~/Content/Falcon/assets/img/icons/paragraph.svg" width="20" alt="" />
                <div class="flex-1">
                    <h5 class="fs-0 d-flex align-items-center">@Resource.NavigationPosition </h5>
                    <p class="fs--1 mb-2">@Resource.SelectNavigation </p>
                    <div>
                        <div class="form-check form-check-inline"><input class="form-check-input" id="option-navbar-vertical" type="radio" name="navbar" value="vertical" data-theme-control="navbarPosition" /><label class="form-check-label" for="option-navbar-vertical">@Resource.Vertical</label></div>
                        <div class="form-check form-check-inline"><input class="form-check-input" id="option-navbar-top" type="radio" name="navbar" value="top" data-theme-control="navbarPosition" /><label class="form-check-label" for="option-navbar-top">@Resource.Top</label></div>
                        <div class="form-check form-check-inline me-0"><input class="form-check-input" id="option-navbar-combo" type="radio" name="navbar" value="combo" data-theme-control="navbarPosition" /><label class="form-check-label" for="option-navbar-combo">@Resource.Combo</label></div>
                    </div>
                </div>
            </div>
            <hr />
            <h5 class="fs-0 d-flex align-items-center">@Resource.VerticalNavbarStyle</h5>
            <p class="fs--1 mb-0">@Resource.SwitchVerticalNavbar </p>
            <div class="btn-group d-block w-100 btn-group-navbar-style">
                <div class="row gx-2">
                    <div class="col-6"><input class="btn-check" id="navbar-style-transparent" type="radio" name="navbarStyle" value="transparent" data-theme-control="navbarStyle" /><label class="btn d-block w-100 btn-navbar-style fs--1" for="navbar-style-transparent"> <img class="img-fluid img-prototype" src="~/Content/Falcon/assets/img/generic/default.png" alt="" /><span class="label-text"> @Resource.Transparent</span></label></div>
                    <div class="col-6"><input class="btn-check" id="navbar-style-inverted" type="radio" name="navbarStyle" value="inverted" data-theme-control="navbarStyle" /><label class="btn d-block w-100 btn-navbar-style fs--1" for="navbar-style-inverted"> <img class="img-fluid img-prototype" src="~/Content/Falcon/assets/img/generic/inverted.png" alt="" /><span class="label-text"> @Resource.Inverted</span></label></div>
                    <div class="col-6"><input class="btn-check" id="navbar-style-card" type="radio" name="navbarStyle" value="card" data-theme-control="navbarStyle" /><label class="btn d-block w-100 btn-navbar-style fs--1" for="navbar-style-card"> <img class="img-fluid img-prototype" src="~/Content/Falcon/assets/img/generic/card.png" alt="" /><span class="label-text"> @Resource.Card</span></label></div>
                    <div class="col-6"><input class="btn-check" id="navbar-style-vibrant" type="radio" name="navbarStyle" value="vibrant" data-theme-control="navbarStyle" /><label class="btn d-block w-100 btn-navbar-style fs--1" for="navbar-style-vibrant"> <img class="img-fluid img-prototype" src="~/Content/Falcon/assets/img/generic/vibrant.png" alt="" /><span class="label-text"> @Resource.Vibrant</span></label></div>
                </div>
            </div>
        </div>
